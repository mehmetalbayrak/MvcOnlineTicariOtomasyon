@model MvcOnlineTicariOtomasyon.Models.Classes.Departman 
@{
    ViewBag.Title = "AddDepartman";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>AddDepartman</h2>
<form class="form-group" method="post">
    @Html.LabelFor(x => x.DepartmanName)
    @Html.TextBoxFor(x => x.DepartmanName, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Kaydet</button>

</form>